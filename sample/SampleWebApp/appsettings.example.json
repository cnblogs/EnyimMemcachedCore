{
  "EnyimMemcached": {
    "Servers": [
      {
        "Address": "memcached",
        "Port": 11211
      }
    ],
    "SocketPool": {
      "minPoolSize": 5,
      "maxPoolSize": 25,
      "connectionTimeout": "00:00:15",
      "receiveTimeout": "00:00:15",
      "deadTimeout": "00:00:15",
      "queueTimeout": "00:00:00.150"
    },
    "SuppressException": false,
    "UseSslStream": false,
    "UseIPv6": false,
    "UseLegacyNodeLocator": false,
    "Transcoder": "MessagePackTranscoder",
    "KeyTransformer": "Enyim.Caching.Memcached.SHA1KeyTransformer",
    "Authentication": {
      "Type": "Enyim.Caching.Memcached.PlainTextAuthenticator",
      "Parameters": {
        "zone": "",
        "userName": "username",
        "password": "password"
      }
    }
  },

  "postbodyMemcached": {
    "Servers": [
      {
        "Address": "memcached",
        "Port": 11211
      }
    ]
  },

  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Enyim": "Warning"
    }
  }
}