{
  "enyimMemcached": {
    "Servers": [
      {
        "Address": "memcached",
        "Port": 11211
      }
    ],
    "socketPool": {
      "minPoolSize": 5,
      "maxPoolSize": 10,
      "connectionTimeout": "00:00:15",
      "receiveTimeout": "00:00:15",
      "deadTimeout": "00:00:15",
      "queueTimeout": "00:00:00.150"
    },
    "suppressException": false,
    "UseLegacyNodeLocator": false,
    "Transcoder": "MessagePackTranscoder"
  },

  "postbodyMemcached": {
    "Servers": [
      {
        "Address": "memcached",
        "Port": 11211
      }
    ]
  },

  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Enyim": "Warning"
    }
  }
}