{
  "enyimMemcached": {
    "Servers": [
      {
        "Address": "memcached",
        "Port": 11211
      }
    ],
    "SocketPool": {
      "MinPoolSize": 5,
      "MaxPoolSize": 10,
      "ConnectionTimeout": "00:00:15",
      "ReceiveTimeout": "00:00:15",
      "DeadTimeout": "00:00:15",
      "QueueTimeout": "00:00:00.150"
    },
    "SuppressException": false,
    "UseLegacyNodeLocator": false,
    "Transcoder": "MessagePackTranscoder"
  },

  "PostbodyMemcached": {
    "Servers": [
      {
        "Address": "memcached",
        "Port": 11211
      }
    ]
  },

  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Enyim": "Warning"
    }
  }
}